{
    "name": "ironchoi/eip712-hash-sign",
    "description": "Generate Eip712 Hashes for signing",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ironchoi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ironchoi",
            "email": "ironchoi@163.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "kornrunner/keccak": "^1.1",
        "kornrunner/solidity": "^0.2.0",
        "illuminate/collections": "^8.0|^9.0|^10.9|^11",
        "illuminate/support": "^8.0|^9.0|^10.9|^11",
        "ironchoi/simple-web3-php": "^0.12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5|^9.0",
        "barryvdh/laravel-ide-helper": "^2.13"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    }
}
